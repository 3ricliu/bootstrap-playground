<h1>Sign In</h1>

<form action="<%= users_url %>" method="post">
  <%= render "layouts/auth" %>
  <label>Username
    <input type="text" name="user[username]" value="">
  </label>
  <br>
  <label>Password
    <input type="text" name="user[password]" value="">
  </label>
  <br>
  <input type="submit" value="Sign In">
</form>
